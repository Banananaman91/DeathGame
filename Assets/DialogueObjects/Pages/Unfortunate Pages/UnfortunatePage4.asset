    _triggerEvent: 0
    _eventNum: 0
  - _npcName: Granny
    _messageText: One morning I received a knock at the door
    _npcMood: 
    _responses: []
    _nextMessage: 4
    _triggerEvent: 0
    _eventNum: 0
  - _npcName: Granny
    _messageText: ' After checking to see who it was, I opened it up to two police
      officers.'
    _npcMood: 
